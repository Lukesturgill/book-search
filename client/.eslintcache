],"import React from 'react';\nimport { Jumbotron, Container, CardColumns, Card, Button } from 'react-bootstrap';\n\nimport { GET_ME } from '../utils/queries';\nimport { REMOVE_BOOK } from '../utils/mutations'\nimport Auth from '../utils/auth';\nimport { removeBookId } from '../utils/localStorage';\n\nimport { useMutation, useQuery } from '@apollo/client';\n\nconst SavedBooks = () => {\n  // use this to determine if `useEffect()` hook needs to run again\n  const { loading, data } = useQuery(GET_ME); \n  const userData = data?.user || {};\n  const userDataLength = Object.keys(userData).length;\n  const [removeBook, { error }] = useMutation(REMOVE_BOOK);\n  if(loading){\n    return <h1>loading</h1>\n  } \n  \n  // create function that accepts the book's mongo _id value as param and deletes the book from the database\n  const handleDeleteBook = async (bookId) => {\n    const token = Auth.loggedIn() ? Auth.getToken() : null;\n\n    if (!token) {\n      return false;\n    }\n\n    try {\n      const { data } = await removeBook({\n        variables: { bookId }\n      });\n\n      if (error) {\n        throw new Error('Something went wrong!');\n      }\n      // upon success, remove book's id from localStorage\n      removeBookId(bookId);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  // if data isn't here yet, say so\n  if (!userDataLength) {\n    return <h2>LOADING...</h2>;\n  }\n\n  return (\n    <>\n      <Jumbotron fluid className='text-light bg-dark'>\n        <Container>\n          <h1>Viewing saved books!</h1>\n        </Container>\n      </Jumbotron>\n      <Container>\n        <h2>\n          {userData.savedBooks.length\n            ? `Viewing ${userData.savedBooks.length} saved ${userData.savedBooks.length === 1 ? 'book' : 'books'}:`\n            : 'You have no saved books!'}\n        </h2>\n        <CardColumns>\n          {userData.savedBooks.map((book) => {\n            return (\n              <Card key={book.bookId} border='dark'>\n                {book.image ? <Card.Img src={book.image} alt={`The cover for ${book.title}`} variant='top' /> : null}\n                <Card.Body>\n                  <Card.Title>{book.title}</Card.Title>\n                  <p className='small'>Authors: {book.authors}</p>\n                  <Card.Text>{book.description}</Card.Text>\n                  <Button className='btn-block btn-danger' onClick={() => handleDeleteBook(book.bookId)}>\n                    Delete this Book!\n                  </Button>\n                </Card.Body>\n              </Card>\n            );\n          })}\n        </CardColumns>\n      </Container>\n    </>\n  );\n};\n\nexport default SavedBooks;\n","/Users/familymacbook/Desktop/projects/challenges/21/client/src/pages/SearchBooks.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/components/Navbar.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/components/LoginForm.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/components/SignupForm.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/utils/API.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/utils/auth.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/utils/localStorage.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/utils/mutations.js",[],"/Users/familymacbook/Desktop/projects/challenges/21/client/src/utils/queries.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":30,"column":15,"nodeType":"61","messageId":"62","endLine":30,"endColumn":19},"no-native-reassign",["63"],"no-negated-in-lhs",["64"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]